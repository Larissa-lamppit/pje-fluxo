<?xml version="1.0" encoding="ISO-8859-1"?>

<process-definition xmlns="urn:jbpm.org:jpdl-3.2" name="(PG) Publicações no Dje">
    <description><![CDATA[Código do fluxo: PG_PUBDJE
 
Descrição: Fluxo destinado a publicação de expedientes no diário da justiça eletrônico...]]></description>  
    <!-- SWIMLANES -->
    <swimlane name="Nó de Desvio - (PG) Publicações no Dje">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('-1:1338,1342:1469')}"/>
    </swimlane>
    <swimlane name="Secretaria">
        <assignment pooled-actors="#{localizacaoAssignment.getPooledActors('5:1338,5:5483,6:1469,8:5852,8:5853')}" actor-id="#{actor.id}"/>
    </swimlane>  
    <!-- START-STATE -->
    <start-state name="Início">
        <task name="Tarefa inicial" priority="3"/>
        <transition to="(PG) Diário da Justiça Eletrônico - PUBLICAR" name="(PG) Diário da Justiça Eletrônico - PUBLICAR"/>
    </start-state>  
    <!-- NODES -->
    <task-node end-tasks="true" name="(PG) Diário da Justiça Eletrônico - PUBLICAR">
        <task name="(PG) Diário da Justiça Eletrônico - PUBLICAR" swimlane="Secretaria" priority="3">
            <controller>
                <variable name="WEB-INF_xhtml_flx_paginaExterna" mapped-name="frame:WEB-INF_xhtml_flx_paginaExterna" access="read,write"/>
                <variable name="movimentacaoLote" mapped-name="movimentarLote:movimentacaoLote" access="read,write"/>
            </controller>
        </task>
        <description><![CDATA[*Variáveis*

1. Variável: WEB-INF_xhtml_flx_paginaExterna
   Label: Expedientes
   Escrita: Sim
   Obrig.: Não
   Tipo: Frame

2. Variável: movimentacaoLote
   Label: 
   Escrita: Sim
   Obrig.: Não
   Tipo: Habilitar Movimentação em Lote]]></description>
        <transition to="Término" name="Finalizar"/>
        <transition to="Nó de Desvio - (PG) Publicações no Dje" name="Nó de Desvio - (PG) Publicações no Dje">
            <condition expression="#{true}"/>
        </transition>
        <event type="task-end">
            <action expression="#{tramitacaoProcessualService.apagaVariavelTarefa('pje:flx:paginaExterna')}"/>
        </event>
        <event type="task-assign">
            <action expression="#{ tramitacaoProcessualService.gravaVariavelTarefa('pje:flx:paginaExterna', parametroUtil.getParametro('pje:tjrn:dje:link').concat('expedientes/').concat(tramitacaoProcessualService.obterTokenDJe(tramitacaoProcessualService.recuperaVariavel('processo'), tramitacaoProcessualService.recuperaVariavel('pje:fluxo:deslocamento:orgaoDestino') != null ? tramitacaoProcessualService.recuperaVariavel('pje:fluxo:deslocamento:orgaoDestino') : tramitacaoProcessualService.recuperaProcesso().getOrgaoJulgador().getIdOrgaoJulgador(),authenticator.getIdUsuarioLogado()))) }"/>
        </event>
    </task-node>
    <end-state name="Término"/>
    <task-node end-tasks="true" name="Nó de Desvio - (PG) Publicações no Dje">
        <task name="Nó de Desvio - (PG) Publicações no Dje" swimlane="Nó de Desvio - (PG) Publicações no Dje" priority="3"/>
        <transition to="Término" name="Término"/>
        <transition to="(PG) Diário da Justiça Eletrônico - PUBLICAR" name="(PG) Diário da Justiça Eletrônico - PUBLICAR"/>
    </task-node>  
    <!-- PROCESS-EVENTS -->
    <event type="superstate-enter">
        <script>br.com.infox.ibpm.util.JbpmEvents.raiseEvent(executionContext)</script>
    </event>
